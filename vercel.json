{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "env": {
    "NODE_VERSION": "20.x"
  }
}
